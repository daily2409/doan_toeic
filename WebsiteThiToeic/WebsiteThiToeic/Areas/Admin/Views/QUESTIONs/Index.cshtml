@model IEnumerable<WebsiteThiToeic.Access.EF.QUESTION>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LISTENNING.LISTEN_URL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.READING.CONTENT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CONTENT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANSWER_A)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANSWER_B)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANSWER_C)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANSWER_D)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CORRECT_ANSWER)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LISTENNING.LISTEN_URL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.READING.CONTENT)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.CONTENT)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.ANSWER_A)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.ANSWER_B)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.ANSWER_C)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.ANSWER_D)
        </td>
        <td style="text-align:center;">
            @Html.DisplayFor(modelItem => item.CORRECT_ANSWER)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
